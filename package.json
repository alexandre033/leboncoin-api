{
  "name": "leboncoin-api",
  "version": "0.0.42",
  "description": "leboncoin.fr API",
  "main": "./main.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec  && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-dev": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tdurieux/leboncoin-api"
  },
  "bugs": {
    "url": "https://github.com/tdurieux/leboncoin-api/issues"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-cookiejar-support": "^1.0.0",
    "lodash": "^4.17.15",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.0.6",
    "eslint": "^7.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "prettier": "^2.0.5",
    "should": "^13.2.3"
  },
  "keywords": [
    "le bon coin",
    "leboncoin",
    "leboncoin.fr"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "parser": "flow",
    "semi": true,
    "tabWidth": 2
  },
  "prettierignore": [
    "package.json"
  ],
  "author": "Thomas Durieux",
  "license": "MIT",
  "readmeFilename": "README.md"
}
